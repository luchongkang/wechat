  height:38px !important;
  margin-right: 15px;
}
@media only screen and (max-width: 375px) {
  .card view:first-child {
    margin-left:20px;
  }
}
@media only screen and (max-width: 320px) {
  .card view:first-child {
    margin-left:10px;
  }
  .child {
    margin:5px;
    background: #FFF;
  }
  .btn {
      width: 100px;
      margin:10px;
      border:1px solid #000;
