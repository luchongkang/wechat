    background-color:#0f131a;
    font-size:18px;
    padding-top:3px;
}

.bottom {
    width:auto;
    margin:0 auto;
    background: #202735;
}
.reset {
    color: #1db571;
    margin:12px 0 0 10px;
    font-size:16px;
}
.choseCard {
  background: #5d729c;
}
.chose {
  border: 2px #35C9AE solid;
  color: #35C9AE;
}
.small {
    left:3px;
}
.selected{
  border-color: #1db571;
  box-shadow:0 0 18px rgba(29, 181, 113, 1);
}
.hide span{
  display:none;
}
.none {
    display:none;
}
.loading {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 100;
}
.lose {
  font-size: 16px;
}
.win {
  color: red;
  font-size: 14px;
}
.tie {
   color: #a6e22e;
   font-size: 12px;
}
.count p {
  margin-top:5px;
}
.unknown {
    font-size: 22px;
    color: #B8B8B8;
    padding: 15px 10px 10px 12px !important;
    border-radius: 7%;
    margin: 2px;
    margin-bottom: -1px; 
    background:#FFF;
}
.login::before {
  content: "\f007";
  box-sizing: border-box;
}
.login {
  color: #35C9AE;
  margin: 10px 30px 1px 10px;
}
.forever {
  position: absolute;
  top: 10px;
  right: 5px;
  font-size: 16px;
}
.forever span:nth-child(1){
  color: #35C9AE;
}
.forever span:nth-child(2){
  background: #E98900;
  padding: 2px 5px;
}
.fa {
  font-family: "fa" !important;
  font-size: 16px;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
}
.qcount {
  width: 100%;
  text-align: center;
  background: #34c9ae;
  padding: 10px 0;
  margin-top: 5px;
}
.modal {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0,0,0,0.8);
  z-index: 1000;
  padding-top: 60px;
  overflow-y:scroll;
}
.tip {
  position: relative;
  background: #FFF;
  margin: 0 20px;
  color: #000;
  border-radius: 20px;
  height: 58%;
}
.tip view:nth-child(1){
  text-align:center;
  padding: 20px;
}
.tip view:nth-child(2){
  text-align: center;
  font-weight: 900;
  font-size: 28px;
}
.tip view:nth-child(3){
  text-align: center;
  font-weight: 900;
  font-size: 24px;
  color: #E98900;
  padding-bottom: 20px;
}
.useforever {
 font-weight: 700;
 margin: 0;
 text-align: center;
}
.useforever::before,.useforever::after{
  position: absolute;
  height: 3px;
  width:92px;
  content: "";
  background: #BCC1CC;
  bottom: 177px;
}
.useforever::after {
  right: 17px;
}
.useforever::before {
  left: 17px;
}
.pay,.debao {
  font-size: 24px;
  margin: 10px;
  padding: 10px 15px;
  text-align: center;
  border-radius: 8px;
}
.debao {
  position: relative;
  background: #35C9AE;
  color: #FFF;
  
}
.debao::before {
  content: "\f005";
  font-family: "fa" !important;
  font-size: 12px;
  position: absolute;
  color: #FFD900;
  left: 2px;
  top: 3px;
  background: #E98900;
  z-index: 2;
  border-radius: 8px;
}
.debao view:nth-child(1) {
  position:absolute;
  left:-20px;
  top:-20px;
}
.debao view:nth-child(1):before,.debao view:nth-child(1):after{
	content:"";
	width:0px;
	height:0px;
	border-right:28px solid transparent;
	position:absolute;
}
.debao view:nth-child(1):before{
	border-top:28px solid #E98900;
}
.debao view:nth-child(1):after{
	border-top:28px solid #E98900;
}
.pay {
  background: #87A5E0;
  margin-bottom: 20px;
  color: #FFF;
}
.startUse {
  position: relative;
  background: #FFF;
  color: #333;
  border-radius: 8px;
  padding: 15px 5px;
  margin: 20px 50px;
  text-align: center;
}
@media only screen and (max-width: 375px) {
  .useforever::before,.useforever::after {
    width: 78px;
    bottom:175px;
  }
  .tip {
    height: 64%;
  }
}
@media only screen and (max-width: 360px) { 
  .useforever::before,.useforever::after {
    bottom:155px;
    width: 60px;
  }
  .useforever::before{
    left: 25px;
  }
  .useforever::after {
    right: 25px;
  }
  .pay,.debao {
    font-size:16px;
  }
  .tip {
    height: 64%;
  }
}
@media only screen and (max-width: 320px) { 
  .useforever::before,.useforever::after {
    bottom:90px;
    width: 62px;
  }
  .pay,.debao {
    font-size:16px;
  }
  .tip {
    height: 68%;
  }
  .title {
    padding:12px 0 0 10px;
  }
  .flex-item {
    margin-left: 5px;
  }
  .count {
    padding-left: 30px;
  }
  .reset {
    margin:15px 0 0 10px;
  }
}
.register {
