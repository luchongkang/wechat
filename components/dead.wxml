<template name="dead">
  <view class="dead {{ setp[0] === 4 ? '' : 'none'}}">
    <view class="deadmsg">
      <span wx:for="{{selDead}}" wx:key="index">{{cards[item['id']]}}{{shap[item['c']]}} </span>
    </view>
    <view class="flex DeadCard">
      <span class="deadDesc">Dead Cards</span>

      <view wx:for="{{deadCard}}" wx:key="index" bindtap="selDeadCard" data-lid="{{item}}"  class="flex-item car {{item.s === 1 ? 'yellow' : ''}}">
        <span class="icon {{color[item['c']]}}">{{shape[item['c']]}}</span>
        <span class="number {{color[item['c']]}} {{item['id'] == 4 ? 'small' : ''}}">{{cards[item['id']]}}</span>
      </view> 
    </view>
    <view class="flex btnDead">
        <view bindtap="resetDead">重置</view>
        <view bindtap="okDead" style="background: #35C9AE;">确定</view>
      </view>
  </view>
</template>